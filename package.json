{
  "name": "bettermath",
  "version": "0.0.3",
  "description": "Basic Mathematical Operations for Arrays in Javascript",
  "main": "bettermath.js",
  "scripts": {
    "test": "echo '=> testing' && qunit -c bettermath.js -t test/tests.js",
    "fix": "echo '=> fixing'  && fixmyjs bettermath.js",
    "lint": "echo '=> linting' && jshint *.js",
    "docs": "echo '=> updating documentation' && docco -c custom.css bettermath.js",
    "release": "echo '=> uglifing' && uglifyjs bettermath.js > bettermath.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AABoyles/bettermath.git"
  },
  "author": "Tony Boyles",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AABoyles/bettermath/issues"
  },
  "homepage": "https://github.com/AABoyles/bettermath#README.md",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "docco": "^0.7.0",
    "fixmyjs": "^1.0.3",
    "qunit": "^1.0.0",
    "uglify-es": "^3.0.4"
  }
}
